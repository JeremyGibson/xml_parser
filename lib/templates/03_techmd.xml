<amdSec xmlns="http://www.loc.gov/METS/" ID="amdSec_techMD">
  <techMD ID="techMD_01">
    <mdWrap MDTYPE="PREMIS">
      <xmlData>
        <premis xmlns="http://www.loc.gov/premis/v3" version="3.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
          <object>
            <objectIdentifier>
              <objectIdentifierType valueURI="http://id.loc.gov/vocabulary/identifiers/local">local</objectIdentifierType>
              <objectIdentifierValue>{eaxs_id}</objectIdentifierValue>
            </objectIdentifier>
            <significantProperties>
              <significantPropertiesValue>This file group constitutes the preservation-master for the email account's messages and attachments.</significantPropertiesValue>
            </significantProperties>
          </object>
        </premis>
      </xmlData>
    </mdWrap>
  </techMD>
  <techMD ID="techMD_02">
    <mdWrap MDTYPE="PREMIS">
      <xmlData>
        <premis xmlns="http://www.loc.gov/premis/v3" version="3.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
          <object>
            <objectIdentifier>
              <objectIdentifierType valueURI="http://id.loc.gov/vocabulary/identifiers/local">local</objectIdentifierType>
              <objectIdentifierValue>{eaxs_id}__tagged</objectIdentifierValue>
            </objectIdentifier>
            <significantProperties>
              <significantPropertiesValue>This file group constitutes the access-master for the email account's messages. This group differs from the preservation-master EAXS file/s in that message content was subjected to machine and optional human efforts in order to mark up entities and personally identifiable information.</significantPropertiesValue>
            </significantProperties>
          </object>
        </premis>
      </xmlData>
    </mdWrap>
  </techMD>
</amdSec>