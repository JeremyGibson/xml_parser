xmlns="http://www.loc.gov/premis/v3" version="3.0"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

	<object xsi:type="representation">
		<objectIdentifier>
			<objectIdentifierType valueURI="http://id.loc.gov/vocabulary/identifiers/local">local</objectIdentifierType>
			<objectIdentifierValue>{root/EAXS file}</objectIdentifierValue>
		</objectIdentifier>
		<preservationLevel>
			<preservationLevelValue/>
