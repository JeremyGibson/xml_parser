		</significantProperties>
	</object>

	<object xsi:type="representation">
		<objectIdentifier>
			<objectIdentifierType valueURI="http://id.loc.gov/vocabulary/identifiers/local">local</objectIdentifierType>
			<objectIdentifierValue>{root/EAXS file}/attachments</objectIdentifierValue>
		</objectIdentifier>
		<preservationLevel>
			<preservationLevelValue/>
		</preservationLevel>
		<significantProperties>
			<significantPropertiesType/>
			<significantPropertiesValue/>
		</significantProperties>
	</object>

</premis>